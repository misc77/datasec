                    <div id="content">
                        <form ng-controller="staticDataCtrl" novalidate>
                            <table>
                                <th><h3><p class='title'>{{title}}</p></h3></th>
                                <tr><td colspan="2"><h4>Wollen Sie den Datensatz "{{formData.bezeichnung}}" wirklich löschen?</h4></td></tr>
                                <tr>
                                    <td><button ng-click='delete()' >Löschen</button></td> 
                                    <td><button ng-click='reset()' class="form-control">Abbrechen</button></td>
                                </tr>
                            </table>
                        </form>
                    </div>
