                    <div id="content">
                        <form novalidate>
                            <div class="row">
                                <table>
                                    <tbody>
                                        <tr>
                                            <td><h4 class="title">{{title}}</h4></td>
                                            <td><a class="btn-floating btn-small waves-effect waves-light indigo netvocat" ng-click="reset()"><i class="material-icons">close</i></a></td>                            
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="card blue-grey lighten-5 col s12">
                                <div class="card-content">
                                    <div class="row"><h5>Wollen Sie den Datensatz "{{formData.display}}" wirklich löschen?</h5></div>
                                    <div class="row">
                                        <div class="input-group">
                                            <a class="waves-effect waves-light btn" ng-click="delete()"><i class="material-icons left">backspace</i>Löschen</a>
                                            <a class="waves-effect waves-light btn" ng-click="reset()"><i class="material-icons left">cancel</i>Abbrechen</a>
                                        </div>
                                    </div>
                                </div>    
                            </div>
                        </form>
                    </div>
