                        <form name="datenForm" ng-controller="staticDataCtrl" novalidate>
                            <p class='title'>{{title}}</h3>
                            <div class="input-group">
                                <span class="input-group-addon" id="basic-addon1">Bezeichnung:</span>
                                <input type="text" class="form-control" placeholder="Bezeichnung der Daten" aria-describedby="basic-addon1" name="bezeichnung" ng-model="formData.bezeichnung" required>
                                <div class="help-block" ng-messages="datenForm.bezeichnung.$error" ng-if="datenForm.bezeichnung.$touched">
                                    <p ng-message="required" >Bezeichnung ist ein Pflichtfeld!</p>
                                </div>
                            </div>
                            <div class="input-group">
                                <span class="input-group-addon" id="basic-addon1">Sensibel:</span>
                                <input type="checkbox" class="form-control" aria-describedby="basic-addon1" name="sensibel" ng-model="formData.sensibel">
                            </div>
                            <div class="input-group">
                                <button ng-click='save()' ng-disabled="datenForm.$invalid">Speichern</button>  
                                <button ng-click='reset()'>Schlie√üen</button>
                            </div>
                        </form>

